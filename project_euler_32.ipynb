    "def isPandigital(a, b, c):\n",
    "    return ''.join(sorted(str(a)+str(b)+str(c))) == '123456789'\n",
    "\n",
    "print(sum(set([a*b for a in range(10000) for b in range(a+1, 10000) if isPandigital(a, b, a * b)])))"
   ]
