    "from tqdm.notebook import tqdm\n",
    "result= []\n",
    "for x in tqdm(range(2,2000001)): \n",
    "    isPrime = True\n",
    "    for i in range(2,x):\n",
    "        if x % i == 0:\n",
    "            isPrime = False\n",
    "            break\n",
    "    if isPrime:\n",
    "         result.append(x)\n",
    "print(sum(result))"
