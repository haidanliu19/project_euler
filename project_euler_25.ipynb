    "import time\n",
    " \n",
    "count_t = time.time()\n",
    "i = 1\n",
    "j = 1\n",
    "total = 2\n",
    "while len(str(j)) != 1000:\n",
    "    total += 1\n",
    "    temp = i\n",
    "    i = j\n",
    "    j += temp\n",
    " \n",
